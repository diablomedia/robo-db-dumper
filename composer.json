{
    "name": "diablomedia/robo-db-dumper",
    "description": "Data dumping commands and tasks for Robo Task Runner",
    "type": "robo-tasks",
    "license": "LGPL-3.0-or-later",
    "autoload": {
        "psr-4": {
            "DiabloMedia\\Robo\\": "src"
        }
    },
    "require": {
        "diablomedia/robo-mysqldump-php": "^1.1.0",
        "php": ">=7.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.11",
        "phpstan/phpstan": "^0.9.2"
    },
    "scripts": {
        "check-style": "php-cs-fixer fix --dry-run -vv",
        "fix-style": "php-cs-fixer fix",
        "phpstan-analysis": "phpstan analyse -l 7 -c phpstan.neon src"
    }
}
