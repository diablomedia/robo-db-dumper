{
    "name": "diablomedia/robo-db-dumper",
    "description": "Data dumping commands and tasks for Robo Task Runner",
    "type": "robo-tasks",
    "license": "LGPL-3.0-or-later",
    "autoload": {
        "psr-4": {
            "DiabloMedia\\Robo\\": "src"
        }
    },
    "require": {
        "diablomedia/robo-mysqldump-php": "^1.1.0",
        "php": ">=7.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "3.54.0",
        "phpstan/phpstan": "^1.10.0"
    },
    "scripts": {
        "style-check": "php-cs-fixer fix --dry-run -vv",
        "fix-style": "php-cs-fixer fix",
        "phpstan": "phpstan analyse -c phpstan.neon --ansi"
    }
}
